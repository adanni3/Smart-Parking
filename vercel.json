{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "index.js": {
      "includeFiles": [
        "wasm-models/lotA-model/run-impulse.js",
        "wasm-models/lotB-model/run-impulse.js"
      ]
    }
  }
}

  